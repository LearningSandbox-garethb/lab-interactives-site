{
  "title": "Water 8",
  "publicationStatus": "draft",
  "subtitle": "",
  "helpOnLoad": true,
  "about": "",
  "theme": ["has"],
  "aspectRatio": 1.4,
  "fontScale": 0.9,
  "models": [
    {
      "type": "iframe-model",
      "id": "iframe-model",
      "url": "models/lab-version/1/iframe-model/iframe-model.json",
      "modelOptions":{
        "url": "//concord-consortium.github.io/agentscript-models-dev/water/lab-embeddable",
        "template": "state/low-res/WaterModel-Gaining-Losing-Stream-StreamA.json",
        "rainProbability": 0,
        "evapProbability": 0,
        "rainCountOptions":  {
          "x": 10,
          "y": 5,
          "dx": 50,
          "dy": 30,
          "debug": false
        },
        "controls": {
          "irrigationWell": true,
          "remove": "well"
        }
      },
      "viewOptions": {
        "aspectRatio": 3.25,
        "controlButtons": "play_reset"
      },
      "onLoad": [
      ]
    }
  ],
  "parameters": [
  ],
  "outputs": [
    {
      "name": "year",
      "value": "return iframeOutput('month') / 12;"
    },
    {
      "name": "rainCount",
      "value": "return iframeOutput('rainCount') || 28"
    }
  ],
  "helpTips": [
  ],
  "components": [
    {
      "id": "water-level-graph",
      "type": "graph",
      "properties": [
        "rainCount"
      ],
      "xProperty": "year",
      "title": "Water Level",
      "width": "100%",
      "height": "100%",
      "xlabel": "Time (years)",
      "xmin": 0,
      "xmax": 40,
      "ylabel": "Water Level",
      "ymin": 0,
      "ymax": 1200,
      "xTickCount": 6,
      "yTickCount": 6,
      "xFormatter": "d",
      "yFormatter": ".2r",
      "dataColors": [
        "#0000ff"
      ]
    }
  ],
  "template": [
    {
      "id": "bottom-left",
      "left": "model.left",
      "width": "model.width * 0.4",
      "top": "model.bottom",
      "bottom": "interactive.bottom"
    },
    {
      "id": "bottom-right",
      "right": "model.right",
      "width": "7em",
      "top": "model.bottom",
      "padding-right": "0.5em",
      "padding-top": "1.5em"
    },
    {
      "id": "right-padding",
      "width": "0.7em",
      "left": "model.right",
      "top": "model.top"
    }
  ],
  "layout": {
    "bottom-left": ["water-level-graph"]
  }
}
