{
  "title": "Water 4",
  "publicationStatus": "draft",
  "subtitle": "",
  "helpOnLoad": true,
  "about": "",
  "theme": ["has", "graph-title-bg", "standalone-playback", "no-slider-bar"],
  "aspectRatio": 1.5,
  "fontScale": 0.8,
  "models": [
    {
      "type": "iframe-model",
      "id": "iframe-model",
      "url": "models/lab-version/1/iframe-model/iframe-model.json",
      "modelOptions":{
        "url": "//models-resources.concord.org/agentscript/water/lab-embeddable.html#xMax=40",
        "template": "state/low-res/WaterModel-5-23-11-confinedAndUnconfined.json",
        "rainProbability": 0.4,
        "evapProbability": 20,
        "controls": {
          "wells": "removal",
          "remove": "well"
        }
      },
      "viewOptions": {
        "aspectRatio": 1.7,
        "controlButtons": "play_reset"
      },
      "onLoad": [
        "function disableComponents(value) {",
        "  setComponentDisabled('followRainButton', value);",
        "}",
        "disableComponents(true);",
        "onStart(function() { disableComponents(false); });",
        "onStop(function() { disableComponents(true); });"
      ]
    }
  ],
  "parameters": [
    {
      "name": "followRain",
      "initialValue": false,
      "onChange": [
        "// Note that we have to use .post() instead of method shortcuts, as this handler is",
        "// called before iframe can register its methods (before iframe communication is started).",
        "if (value) {",
        "  post('addRainSpotlight');",
        "  setComponentAttributes('followRainButton', {text: 'Stop following'});",
        "} else {",
        "  post('removeSpotlight');",
        "  setComponentAttributes('followRainButton', {text: 'Follow water droplet'});",
        "}"
      ]
    }
  ],
  "outputs": [
    {
      "name": "year",
      "value": "return iframeOutput('month') / 12;"
    },
    {
      "name": "well0",
      "value": "return iframeOutput('well0') || 0"
    },
    {
      "name": "well1",
      "value": "return iframeOutput('well1') || 0"
    },
    {
      "name": "well2",
      "value": "return iframeOutput('well2') || 0"
    },
    {
      "name": "well3",
      "value": "return iframeOutput('well3') || 0"
    },
    {
      "name": "well4",
      "value": "return iframeOutput('well4') || 0"
    }
  ],
  "helpTips": [
  ],
  "components": [
    {
      "type": "playback",
      "id": "playback"
    },
    {
      "id": "followRainButton",
      "type": "button",
      "action": [
        "set('followRain', !get('followRain'));"
      ],
      "text": "Follow water droplet",
      "width": "14em"
    },
    {
      "id": "rainSlider",
      "type": "slider",
      "min": 0,
      "max": 1,
      "title": "Rain probability",
      "fillColor": "#459ACE",
      "width": "15em",
      "steps": 50,
      "labels": [
        {
          "value": "0",
          "label": "<i class='fa fa-sun-o' style='font-size: 1.5em; margin-right: 0.4em;'></i>"
        },
        {
          "value": "1",
          "label": "<i class='fa fa-umbrella' style='font-size: 1.5em;'></i>"
        }
      ],
      "property": "rainProbability"
    },
    {
      "id": "wells-graph",
      "type": "graph",
      "properties": [
        "well0",
        "well1",
        "well2",
        "well3",
        "well4"
      ],
      "enableZooming": false,
      "enableAxisScaling": false,
      "xProperty": "year",
      "title": "Well output",
      "titlePosition": "left",
      "width": "100%",
      "height": "100%",
      "xlabel": "Time (years)",
      "xmin": 0,
      "xmax": 10,
      "ylabel": "Well output",
      "ymin": 0,
      "ymax": 600,
      "xTickCount": 6,
      "yTickCount": 6,
      "xFormatter": "d",
      "yFormatter": ".2r",
      "dataColors": [
        "#0000FF",
        "#FF0066",
        "#000000",
        "#CC33FF",
        "#FF6600"
      ],
      "legendLabels": [
        "Well 1 Output",
        "Well 2 Output",
        "Well 3 Output",
        "Well 4 Output",
        "Well 5 Output"
      ]
    }
  ],
  "template": [
    {
      "id": "right",
      "left": "model.right",
      "width": "model.width * 0.6",
      "top": "model.top",
      "bottom": "model.bottom",
      "padding": "0 0.5em"
    },
    {
      "id": "bottom",
      "top": "model.bottom",
      "left": "model.left",
      "margin-top": "0.3em"
    }
  ],
  "layout": {
    "bottom": [
      ["playback"],
      ["followRainButton"],
      ["rainSlider"]
    ],
    "right": [
      "wells-graph"
    ]
  }
}
