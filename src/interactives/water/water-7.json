{
  "title": "Water 7",
  "publicationStatus": "draft",
  "subtitle": "",
  "helpOnLoad": true,
  "about": "",
  "theme": ["has"],
  "aspectRatio": 1.3,
  "fontScale": 0.8,
  "models": [
    {
      "type": "iframe-model",
      "id": "iframe-model",
      "url": "models/lab-version/1/iframe-model/iframe-model.json",
      "modelOptions":{
        "url": "//concord-consortium.github.io/agentscript-models-dev/water/lab-embeddable#xMax=125&xMin=-125",
        "template": "state/low-res/WaterModel-Gaining-Losing-Stream-StreamA.json",
        "rainProbability": 0,
        "evapProbability": 0,
        "rainCountOptions":  {
          "x": 10,
          "y": 5,
          "dx": 50,
          "dy": 30,
          "debug": false
        }
      },
      "viewOptions": {
        "aspectRatio": 1.8,
        "controlButtons": "play_reset"
      },
      "onLoad": [
        "onPropertyChange('template', function() {",
        "  reloadModel();",
        "});"
      ]
    }
  ],
  "parameters": [
  ],
  "outputs": [
    {
      "name": "year",
      "value": "return iframeOutput('month') / 12;"
    },
    {
      "name": "initialRainCount",
      "value": [
        "if (get('template') === 'state/low-res/WaterModel-Gaining-Losing-Stream-StreamA.json') return 28;",
        "if (get('template') === 'state/low-res/WaterModel-Gaining-Losing-Stream-StreamB.json') return 1120;"
      ]
    },
    {
      "name": "rainCount",
      "value": "return iframeOutput('rainCount') || get('initialRainCount');"
    },
    {
      "name": "well0",
      "value": "return iframeOutput('well0') || 0"
    },
    {
      "name": "well1",
      "value": "return iframeOutput('well1') || 0"
    },
    {
      "name": "well2",
      "value": "return iframeOutput('well2') || 0"
    },
    {
      "name": "well3",
      "value": "return iframeOutput('well3') || 0"
    },
    {
      "name": "well4",
      "value": "return iframeOutput('well4') || 0"
    }
  ],
  "helpTips": [
  ],
  "components": [
    {
      "id": "water-level-graph",
      "type": "graph",
      "properties": [
        "rainCount"
      ],
      "enableZooming": false,
      "enableAxisScaling": false,
      "xProperty": "year",
      "title": "Water Level",
      "width": "100%",
      "height": "100%",
      "xlabel": "Time (years)",
      "xmin": 0,
      "xmax": 10,
      "ylabel": "Water Level",
      "ymin": 0,
      "ymax": 1200,
      "xTickCount": 6,
      "yTickCount": 6,
      "xFormatter": "d",
      "yFormatter": ".2r",
      "dataColors": [
        "#0000ff"
      ],
      "syncXAxis": "wells-graph"
    },
    {
      "id": "template",
      "type": "radio",
      "property": "template",
      "options": [
        {
          "text": "Stream A",
          "value": "state/low-res/WaterModel-Gaining-Losing-Stream-StreamA.json"
        },
        {
          "text": "Stream B",
          "value": "state/low-res/WaterModel-Gaining-Losing-Stream-StreamB.json"
        }
      ]
    },
    {
      "id": "wells-graph",
      "type": "graph",
      "properties": [
        "well0",
        "well1",
        "well2",
        "well3",
        "well4"
      ],
      "enableZooming": false,
      "enableAxisScaling": false,
      "xProperty": "year",
      "title": "Well output",
      "width": "100%",
      "height": "100%",
      "xlabel": "Time (years)",
      "xmin": 0,
      "xmax": 10,
      "ylabel": "Well output",
      "ymin": 0,
      "ymax": 600,
      "xTickCount": 6,
      "yTickCount": 6,
      "xFormatter": "d",
      "yFormatter": ".2r",
      "dataColors": [
        "#ff0000",
        "#00ff00",
        "#0000ff",
        "#989898",
        "#000000"
      ],
      "legendLabels": [
        "Well 1 Output",
        "Well 2 Output",
        "Well 3 Output",
        "Well 4 Output",
        "Well 5 Output"
      ],
      "syncXAxis": "water-level-graph"
    },
    {
      "id": "playback",
      "type": "playback"
    }
  ],
  "template": [
    {
      "id": "playback-container",
      "top": "model.bottom",
      "left": "model.left"
    },
    {
      "id": "bottom",
      "top": "playback-container.bottom",
      "left": "model.left",
      "margin-top": "0.5em"
    },
    {
      "id": "right-top",
      "top": "model.top",
      "left": "model.right",
      "width": "model.width * 0.5",
      "height": "model.width * 0.5",
      "margin": "0 0.3em"
    },
    {
      "id": "right-bottom",
      "top": "right-top.bottom",
      "left": "model.right",
      "width": "model.width * 0.5",
      "height": "model.width * 0.5",
      "margin": "0.3em"
    }
  ],
  "layout": {
    "playback-container": [
      "playback"
    ],
    "bottom": [
      "template"
    ],
    "right-top": [
      "wells-graph"
    ],
    "right-bottom": [
      "water-level-graph"
    ]
  }
}
