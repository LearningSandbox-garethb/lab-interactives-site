{
  "title": "Planet Hunting (test)",
  "publicationStatus": "public",
  "theme": [
    "has",
    "standalone-playback",
    "no-slider-bar",
    "graph-title-bg"
  ],
  "aspectRatio": 1.1,
  "fontScale": 0.7,
  "models": [
    {
      "type": "iframe-model",
      "id": "iframe-model",
      "url": "models/lab-version/1/iframe-model/iframe-model.json",
      "modelOptions": {
        "url": "//concord-consortium.github.io/space-models/planet-hunting/",
        "timestep": 0.001,
        "planet.diameter": 1,
        "planet.rocky": true
      },
      "viewOptions": {
        "controlButtons": "play_reset"
      },
      "onLoad": [
      ]
    }
  ],
  "outputs": [
    {
      "name": "planet.mass",
      "value": "return iframeOutput('planet.mass') || 1;"
    },
    {
      "name": "camera.tilt",
      "value": "return iframeOutput('camera.tilt') || 90;"
    }
  ],
  "parameters": [
  ],
  "components": [
    {
      "id": "timestep",
      "type": "slider",
      "min": 0.0002,
      "max": 0.005,
      "steps": 100,
      "title": "Speed",
      "labels": [
        {
          "value": 0.0002,
          "label": "slow"
        },
        {
          "value": 0.005,
          "label": "fast"
        }
      ],
      "property": "timestep"
    },
    {
      "id": "planet-diameter",
      "type": "slider",
      "min": 1,
      "max": 50,
      "steps": 100,
      "title": "Planet diameter",
      "labels": [
        {
          "value": 1,
          "label": "earth x 1"
        },
        {
          "value": 50,
          "label": "earth x 50"
        }
      ],
      "property": "planet.diameter"
    },
    {
      "id": "planet-mass",
      "type": "numericOutput",
      "property": "planet.mass",
      "label": "The planet's mass is",
      "displayValue": "return value.toFixed(2) + ' times the earth mass';"
    },
    {
      "id": "planet-rocky",
      "type": "checkbox",
      "text": "Rocky planet",
      "property": "planet.rocky"
    },
    {
      "id": "camera-tilt",
      "type": "numericOutput",
      "property": "camera.tilt",
      "label": "The tilt is",
      "displayValue": "return value.toFixed(2) + ' degrees';"
    }
  ],
  "template": [
    {
      "id": "bottom-1",
      "left": "model.left",
      "top": "model.bottom"
    },
    {
      "id": "bottom-2",
      "left": "bottom-1.right",
      "top": "model.bottom",
      "margin-left": "2em"
    },
    {
      "id": "right-padding",
      "left": "model.right",
      "padding-right": "1em"
    }
  ],
  "layout": {
    "bottom-1": [
      ["timestep"],
      ["planet-diameter"],
      ["planet-rocky"]
    ],
    "bottom-2": [
      ["camera-tilt"],
      ["planet-mass"]
    ]
  }
}