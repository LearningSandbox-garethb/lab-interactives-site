{
  "title": "Planet Hunting (test)",
  "publicationStatus": "public",
  "theme": [
    "has",
    "standalone-playback",
    "no-slider-bar",
    "graph-title-bg"
  ],
  "aspectRatio": 1.45,
  "fontScale": 0.7,
  "models": [
    {
      "type": "iframe-model",
      "id": "iframe-model",
      "url": "models/lab-version/1/iframe-model/iframe-model.json",
      "modelOptions": {
        "url": "//concord-consortium.github.io/space-models/planet-hunting/",
        "timestep": 0.001,
        "planet.diameter": 1,
        "planet.rocky": true
      },
      "viewOptions": {
        "controlButtons": "play_reset"
      },
      "onLoad": [
      ]
    }
  ],
  "outputs": [
    {
      "name": "time",
      "value": "return iframeOutput('time') * 365 || 0;"
    },
    {
      "name": "starCamVelocity",
      "value": "return iframeOutput('starCamVelocity') * 50000 || 0;"
    },
    {
      "name": "lightIntensity",
      "value": "return iframeOutput('lightIntensity') || 0;"
    },
    {
      "name": "planet.mass",
      "value": "return iframeOutput('planet.mass') || 0;"
    },
    {
      "name": "camera.tilt",
      "value": "return iframeOutput('camera.tilt') || 0;"
    }
  ],
  "parameters": [
  ],
  "components": [
    {
      "id": "timestep",
      "type": "slider",
      "min": 0.0002,
      "max": 0.005,
      "steps": 100,
      "title": "Speed",
      "labels": [
        {
          "value": 0.0002,
          "label": "slow"
        },
        {
          "value": 0.005,
          "label": "fast"
        }
      ],
      "property": "timestep"
    },
    {
      "id": "planet-diameter",
      "type": "slider",
      "min": 1,
      "max": 50,
      "steps": 100,
      "title": "Planet diameter",
      "labels": [
        {
          "value": 1,
          "label": "earth x 1"
        },
        {
          "value": 50,
          "label": "earth x 50"
        }
      ],
      "property": "planet.diameter"
    },
    {
      "id": "planet-mass",
      "type": "numericOutput",
      "property": "planet.mass",
      "label": "The planet's mass is",
      "displayValue": "return value.toFixed(2) + ' times the earth mass';"
    },
    {
      "id": "planet-rocky",
      "type": "checkbox",
      "text": "Rocky planet",
      "property": "planet.rocky"
    },
    {
      "id": "camera-tilt",
      "type": "numericOutput",
      "property": "camera.tilt",
      "label": "The tilt is",
      "displayValue": "return value.toFixed(2) + ' degrees';"
    },
    {
      "id": "star-cam-velocity",
      "type": "graph",
      "properties": [
        "starCamVelocity"
      ],
      "xProperty": "time",
      "title": "Velocity of star relative to observer",
      "titlePosition": "left",
      "buttonsLayout": "horizontal",
      "buttonsStyle": "text",
      "lineWidth": 2,
      "xlabel": "Time (days)",
      "xmin": 0,
      "xmax": 600,
      "ylabel": "Velocity",
      "ymin": -1,
      "ymax": 1,
      "xTickCount": 4,
      "yTickCount": 5,
      "dataColors": [
        "#ff0000"
      ]
    },
    {
      "id": "light-intensity",
      "type": "graph",
      "properties": [
        "lightIntensity"
      ],
      "xProperty": "time",
      "title": "Light intensity",
      "titlePosition": "left",
      "buttonsLayout": "horizontal",
      "buttonsStyle": "text",
      "lineWidth": 2,
      "xlabel": "Time (days)",
      "xmin": 0,
      "xmax": 600,
      "ylabel": "Velocity",
      "ymin": 0.7,
      "ymax": 1.02,
      "xTickCount": 4,
      "yTickCount": 5,
      "dataColors": [
        "#ff0000"
      ]
    }
  ],
  "template": [
    {
      "id": "bottom-1",
      "left": "model.left",
      "top": "model.bottom"
    },
    {
      "id": "bottom-2",
      "left": "bottom-1.right",
      "top": "model.bottom",
      "margin-left": "2em"
    },
    {
      "id": "right-top",
      "left": "model.right",
      "height": "model.height * 0.5",
      "width": "20em",
      "padding-left": "0.3em",
      "padding-right": "0.3em"
    },
    {
      "id": "right-bottom",
      "left": "model.right",
      "top": "right-top.bottom",
      "height": "model.height * 0.5",
      "width": "20em",
      "padding-left": "0.3em",
      "padding-right": "0.3em"
    }
  ],
  "layout": {
    "bottom-1": [
      ["timestep"],
      ["planet-diameter"],
      ["planet-rocky"]
    ],
    "bottom-2": [
      ["camera-tilt"],
      ["planet-mass"]
    ],
    "right-top": [
      "star-cam-velocity"
    ],
    "right-bottom": [
      "light-intensity"
    ]
  }
}