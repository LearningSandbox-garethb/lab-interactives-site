{
  "title": "Planet Hunting (test)",
  "publicationStatus": "public",
  "theme": [
    "has",
    "standalone-playback",
    "no-slider-bar",
    "graph-title-bg"
  ],
  "aspectRatio": 1.05,
  "fontScale": 0.7,
  "models": [
    {
      "type": "iframe-model",
      "id": "iframe-model",
      "url": "models/lab-version/1/iframe-model/iframe-model.json",
      "modelOptions": {
        "url": "//concord-consortium.github.io/space-models/planet-hunting/",
        "timestep": 0.001,
        "planet.diameter": 1,
        "planet.rocky": true,
        "telescope.precision": 100
      },
      "viewOptions": {
        "controlButtons": "play_reset"
      },
      "onLoad": [
      ]
    }
  ],
  "outputs": [
    {
      "name": "time",
      "value": "return iframeOutput('time') * 365 || 0;"
    },
    {
      "name": "starCamVelocity",
      "value": "return iframeOutput('telescope.starCamVelocity') || 0;"
    },
    {
      "name": "lightIntensity",
      "value": "return iframeOutput('telescope.lightIntensity') || 0;"
    },
    {
      "name": "planet.mass",
      "value": "return iframeOutput('planet.mass') || 0;"
    },
    {
      "name": "camera.tilt",
      "value": "return iframeOutput('camera.tilt') || 0;"
    }
  ],
  "parameters": [
  ],
  "components": [
    {
      "id": "circular-orbit",
      "type": "button",
      "text": "Make circular orbit",
      "action": "post('makeCircularOrbit');"
    },
    {
      "id": "clear-graphs",
      "type": "button",
      "text": "Clear all graphs",
      "action": [
        "var newXMin = Math.round(get('time'));",
        "var newXMax = newXMin + 600;",
        "setComponentAttributes('star-cam-velocity', {xmin: newXMin, xmax: newXMax});",
        "setComponentAttributes('light-intensity', {xmin: newXMin, xmax: newXMax});"
      ]
    },
    {
      "id": "timestep",
      "type": "slider",
      "fillColor": "#2DA343",
      "min": 0.0002,
      "max": 0.005,
      "steps": 100,
      "title": "Speed",
      "labels": [
        {
          "value": 0.0002,
          "label": "slow"
        },
        {
          "value": 0.005,
          "label": "fast"
        }
      ],
      "property": "timestep"
    },
    {
      "id": "planet-diameter",
      "type": "slider",
      "fillColor": "#459ACE",
      "min": 1,
      "max": 50,
      "steps": 100,
      "title": "Planet diameter",
      "labels": [
        {
          "value": 1,
          "label": "1X Earth"
        },
        {
          "value": 50,
          "label": "50X Earth"
        }
      ],
      "property": "planet.diameter"
    },
    {
      "id": "telescope-precision",
      "type": "slider",
      "fillColor": "#459ACE",
      "min": 5,
      "max": 100,
      "steps": 100,
      "title": "Telescope precision",
      "labels": [
        {
          "value": 5,
          "label": "5%"
        },
        {
          "value": 100,
          "label": "100%"
        }
      ],
      "property": "telescope.precision"
    },
    {
      "id": "planet-mass",
      "type": "numericOutput",
      "property": "planet.mass",
      "label": "Planet mass:",
      "displayValue": "return value.toFixed(0) + 'x Earth';"
    },
    {
      "id": "planet-rocky",
      "type": "checkbox",
      "text": "Rocky planet",
      "property": "planet.rocky"
    },
    {
      "id": "star-cam-velocity",
      "type": "graph",
      "enableAxisScaling": false,
      "properties": [
        "starCamVelocity"
      ],
      "xProperty": "time",
      "title": "Velocity of Star, Relative to Observer",
      "titlePosition": "left",
      "buttonsLayout": "horizontal",
      "buttonsStyle": "text",
      "lineWidth": 2,
      "xlabel": "Time (days)",
      "xmin": 0,
      "xmax": 600,
      "ylabel": "Velocity",
      "ymin": -2.5,
      "ymax": 2.5,
      "xTickCount": 8,
      "yTickCount": 4,
      "dataColors": [
        "#0000ff"
      ],
      "syncXAxis": "light-intensity"
    },
    {
      "id": "light-intensity",
      "type": "graph",
      "enableAxisScaling": false,
      "properties": [
        "lightIntensity"
      ],
      "xProperty": "time",
      "title": "Light Intensity of Star",
      "titlePosition": "left",
      "buttonsLayout": "horizontal",
      "buttonsStyle": "text",
      "lineWidth": 2,
      "xlabel": "Time (days)",
      "xmin": 0,
      "xmax": 600,
      "ylabel": "Light Intensity",
      "ymin": 0.7,
      "ymax": 1.02,
      "xTickCount": 8,
      "yTickCount": 4,
      "dataColors": [
        "#0000ff"
      ],
      "syncXAxis": "star-cam-velocity"
    },
        {
      "id": "playback",
      "type": "playback",
      "stepping": true
    }
  ],
 "template": [
     {
      "id": "tilt-container",
      "top": "model.bottom",
      "left": "model.left",
      "padding-top": "0.75em",
      "padding-right": "0.5em"
    },
          {
      "id": "playback-container",
      "top": "tilt-container.bottom",
      "left": "model.left",
      "padding-top": "0.75em",
      "padding-right": "0.5em"
    },
        {
      "id": "playback-right",
      "top": "tilt-container.bottom",
      "left": "playback-container.right",
      "padding-bottom": "0.5em"
    },
    {
      "id": "bottom-1",
      "left": "model.left",
      "top": "bottom-1b.bottom"
    },
       {
      "id": "bottom-1b",
      "left": "model.left",
      "top": "playback-right.bottom"
    },
    {
      "id": "bottom-2",
      "left": "bottom-1.right",
      "top": "bottom-1.top",
      "margin-left": "2em"
    },
    {
      "id": "bottom-3",
      "left": "right-bottom.left",
      "top": "right-bottom.bottom",
      "align": "right",
      "margin-top": "1em",
      "margin-right": "1em"
    },
    {
      "id": "right-top",
      "left": "model.right",
      "height": "interactive.height * 0.44",
  	  "width": "20em",
      "padding-left": "0.3em",
      "padding-right": "0.3em"
    },
    {
      "id": "right-bottom",
      "left": "model.right",
      "top": "right-top.bottom",
      "height": "interactive.height * 0.44",
      "width": "20em",
      "padding-left": "0.3em",
      "padding-right": "0.3em"
    }
  ],
  "layout": {
    "playback-container": [
      [
        "playback"
      ]
    ],
    "playback-right": [
      "timestep"
    ],
    "bottom-1": [
      [
        "planet-diameter"
      ],
      [
        "telescope-precision"
      ],
      [
        "circular-orbit"
      ]
    ],
    "bottom-1b": [
      [
        "planet-rocky"
      ]
    ],
    "bottom-2": [
      [
        "planet-mass"
      ]
    ],
    "bottom-3": [
      [
        "clear-graphs"
      ]
    ],
    "right-top": [
      "star-cam-velocity"
    ],
    "right-bottom": [
      "light-intensity"
    ]
  },
  "template": [
    {
      "id": "playback-container",
      "top": "model.bottom",
      "left": "model.left",
      "padding-top": "0.75em",
      "padding-right": "0.5em"
    },
    {
      "id": "playback-right",
      "top": "model.bottom",
      "left": "playback-container.right",
      "padding-bottom": "0.5em"
    },
    {
      "id": "bottom-1",
      "left": "model.left",
      "top": "bottom-1b.bottom"
    },
    {
      "id": "bottom-1b",
      "left": "model.left",
      "top": "playback-right.bottom"
    },
    {
      "id": "bottom-2",
      "left": "bottom-1.right",
      "top": "bottom-1.top",
      "margin-left": "2em"
    },
    {
      "id": "bottom-3",
      "left": "right-bottom.left",
      "top": "right-bottom.bottom",
      "align": "right",
      "margin-top": "1em",
      "margin-right": "1em"
    },
    {
      "id": "right-top",
      "left": "model.right",
      "height": "interactive.height * 0.44",
      "width": "20em",
      "padding-left": "0.3em",
      "padding-right": "0.3em"
    },
    {
      "id": "right-bottom",
      "left": "model.right",
      "top": "right-top.bottom",
      "height": "interactive.height * 0.44",
      "width": "20em",
      "padding-left": "0.3em",
      "padding-right": "0.3em"
    }
  ]
}
