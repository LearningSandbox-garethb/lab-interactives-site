{
  "title": "Air Pollution 2",
  "publicationStatus": "draft",
  "subtitle": "Formation of Secondary Pollutants",
  "helpOnLoad": true,
  "about": "Discover the connection between weather, geography, and air quality. Explore the combination of factors that cause smog (secondary pollutants, such as ozone) to develop over a city.",
  "theme": ["has","standalone-playback","no-slider-bar"],
  "aspectRatio": 1.3,
  "fontScale": 0.85,
  "models": [
    {
      "type": "iframe-model",
      "id": "iframe-model",
      "url": "models/lab-version/1/iframe-model/iframe-model.json",
      "modelOptions":{
        "url": "//models-resources.concord.org/agentscript/air-pollution/lab-embeddable.html",
        "includeSunlight": true,
        "sunlightAmount": 6,
        "rainRate": 3,
        "windSpeed": 0,
        "numCars": 2,
        "numFactories": 2,
        "carPollutionRate": 200,
        "factoryPollutionRate": 300
      },
      "viewOptions": {
        "controlButtons": "play_reset"
      },
      "onLoad": []
    }
  ],
  "parameters": [
  ],
  "outputs": [
    {
      "name": "ticks",
      "value": "return iframeOutput('ticks');"
    },
    {
      "name": "primaryAQI",
      "value": "return iframeOutput('primaryAQI');"
    },
    {
      "name": "secondaryAQI",
      "value": "return iframeOutput('secondaryAQI');"
    }
  ],
  "helpTips": [
    {
      "component": "",
      "text": [
        "#How do I use this model?",
        "---",
        "Press the play button to start the model.",
        "",
        "",
        "Change the wind speed and direction, amount of sunlight, and rain frequency to discover what conditions are favorable for smog formation.",
        "",
        "",
        "You can hover over the controls for more information. Click to close this window."
      ],
      "showcase": true
    }
  ],
  "components": [
    {
      "id": "aqiGraph",
      "type": "graph",
      "resetAxesOnReset": true,
      "enableAutoScaleButton": true,
      "enableAxisScaling": false,
      "enableZooming": false,
      "autoScaleX": true,
      "autoScaleY": true,
      "enableSelectionButton": false,
      "clearSelectionOnLeavingSelectMode": false,
      "enableDrawButton": false,
      "drawProperty": null,
      "markAllDataPoints": false,
      "showRulersOnSelection": false,
      "fontScaleRelativeToParent": true,
      "properties": [
        "primaryAQI",
        "secondaryAQI"
      ],
      "xProperty": "ticks",
      "title": "Air Pollutants",
      "lineWidth": 2,
      "width": "100%",
      "height": "100%",
      "xlabel": "Time (ticks)",
      "xmin": 0,
      "xmax": 100,
      "ylabel": "AQI (Air Quality Index)",
      "ymin": 0,
      "ymax": 300,
      "xTickCount": 4,
      "yTickCount": 6,
      "xscaleExponent": 0.5,
      "yscaleExponent": 0.5,
      "xFormatter": "0f",
      "yFormatter": "0f",
      "lines": true,
      "bars": false,
      "dataColors": [
        "#664935",
        "#F47921",
        "#000000"
      ],
      "legendLabels": [
          "Primary Pollutants",
          "Secondary Pollutants"
      ],
      "legendVisible": true,
      "syncXAxis": false,
      "syncYAxis": false,
      "helpIcon": false
    },
    {
      "id": "windSlider",
      "type": "slider",
      "min": -100,
      "max": 100,
      "steps": 10,
      "title": "Wind",
      "labels": [
        {
          "value": "left",
          "label": "<i class='icon-arrow-left'></i>"
        },
        {
          "value": 0,
          "label": "None"
        },
        {
          "value": "right",
          "label": "<i class='icon-arrow-right'></i>"
        }
      ],
      "width": "18em",
      "height": "auto",
      "property": "windSpeed",
      "retainProperty": true,
      "disabled": false,
      "tooltip": "Change the speed and direction of the wind."
    },
    {
      "id": "sunSlider",
      "type": "slider",
      "min": 0,
      "max": 10,
      "steps": 10,
      "title": "Sunlight",
      "fillColor": "#459ACE",
      "labels": [
        {
          "value": "left",
          "label": "<i class='icon-minus'></i>"
        },
        {
          "value": "right",
          "label": "<i class='icon-plus'></i><i class='icon-plus'></i><i class='icon-plus'></i>"
        }
      ],
      "width": "18em",
      "height": "auto",
      "property": "sunlightAmount",
      "retainProperty": true,
      "disabled": false,
      "tooltip": "Change the amount of sunlight."
    },
    {
      "id": "rainSlider",
      "type": "slider",
      "min": 0,
      "max": 6,
      "steps": 6,
      "title": "Rain",
      "fillColor": "#459ACE",
      "labels": [
        {
          "value": "left",
          "label": "<i class='icon-minus'></i>"
        },
        {
          "value": "right",
          "label": "<i class='icon-plus'></i><i class='icon-plus'></i><i class='icon-plus'></i>"
        }
      ],
      "width": "18em",
      "height": "auto",
      "property": "rainRate",
      "retainProperty": true,
      "disabled": false,
      "tooltip": "Change the amount of rain."
    },
    {
      "id": "playback",
      "type": "playback",
      "stepping": true
    }
  ],
 "layout": {
    "playback-container": [
      "playback"
    ],
    
    "bottomLeft": [
      [
        "windSlider"
      ],
        [
        "sunSlider"
      ],
      [
        "rainSlider"
      ]
    ],
    "topRight": [
      [
        "aqiGraph"
      ]
    ]
  },
  "template": [
    {
      "id": "playback-container",
      "top": "model.bottom",
      "left": "model.left"
    
    },
    
    {
      "id": "bottomLeft",
      "top": "playback-container.bottom",
      "left": "model.left"
    },

    {
      "id": "topRight",
      "top": "model.top",
      "left": "model.right",
      "width": "0.75 * model.width",
      "height": "model.height",
      "padding-left": "0.5em"
    }
  ]
}
